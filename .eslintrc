{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "extends": [
    "eslint-config-google",
    "eslint:recommended",
    "plugin:flowtype/recommended",
    "plugin:jest/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "impliedStrict": true
    },
    "ecmaVersion": 7,
    "sourceType": "script"
  },
  "plugins": [
    "jest",
    "flowtype"
  ],
  "rules": {
    "linebreak-style": 0,
    "flowtype/no-weak-types": 1,
    "no-console": 0,
  }
}
